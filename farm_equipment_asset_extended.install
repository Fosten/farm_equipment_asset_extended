<?php

/**
 * Implements hook_uninstall().
 */

 function farm_equipment_asset_extended_uninstall() {
     // Uninstall the farm_equipment_type module.
   if (\Drupal::service('module_handler')->moduleExists('farm_equipment_type')){
     \Drupal::service('module_installer')->uninstall(['farm_equipment_type']);
   }
 }